@model IEnumerable<Team>;
@{
    ViewData["Title"] = "Team";
}
<!-- ======= TeamMember Section ======= -->
<section id="services" class="services">
    <div class="container" data-aos="fade-up">

        <div class="section-title">
            <h2>TeamMember</h2>
        </div>
        @if (Model.Count() > 0)
        {
            <div class="row">

                @foreach (var item in Model)
                {

                    <div class="col-md-3"style="cursor:pointer;background:#f7f7f7;padding:2%">
                        <div class="thumbnail">
                            <img src="~/assets/img/doctors/doctors-1.jpg" class="img-fluid" alt="Lights" style="width:100%">
                            <div class="caption text-center" style="background: #0268be">
                                <p style="font-size: large;color: #fff;line-height: 3rem">@item.Name</p>
                                <p style="color: #fff;">@item.JopTitle</p>

                            </div>

                        </div>
                    </div>
                }
            </div>
        }
    </div>
</section><!-- End Services Section -->

